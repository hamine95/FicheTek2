    <Grid KeyboardNavigation.DirectionalNavigation="Contained" Focusable="True">
        <Grid.InputBindings>
            <KeyBinding Modifiers="Ctrl" Key="Down" Command="{Binding SecondPageCmd}" />
            <KeyBinding Modifiers="Ctrl" Key="Up" Command="{Binding FirstPageCmd}" />
        </Grid.InputBindings>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2.2*" />
            <ColumnDefinition Width="8*" />
            <ColumnDefinition Width="40" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>


        <Grid  
            KeyboardNavigation.TabNavigation="None"
            CustClass:perSizeBindingHelper.Active="True"
            CustClass:perSizeBindingHelper.BoundActualWidth="{Binding PanelWidth, Mode=OneWayToSource}"

            >
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <RowDefinition Height="auto" />
                <RowDefinition Height="auto" />
                <RowDefinition Height="auto" />
                <RowDefinition />
                <RowDefinition Height="auto" />
            </Grid.RowDefinitions>


            <DockPanel IsEnabled="{Binding DisplayDate}" Height="30" Grid.Row="1" LastChildFill="True">

                <CustClass:GitHubButton Visibility="Collapsed" Cursor="Hand" Width="30"
                                        Style="{StaticResource SearchBtn}" DockPanel.Dock="Right" />
                <TextBox Text="{Binding SearchText,UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center"
                         FontSize="16" HorizontalAlignment="Stretch" DockPanel.Dock="Left"
                         Height="25"
                         Margin="5 0 5 0"
                         BorderBrush="Black"
                         ToolTip="Rechercher Fiche Technique">
                    
                </TextBox>
            </DockPanel>
            <StackPanel Margin="0 0 0 10" Grid.Row="2" Orientation="Horizontal">
                <CheckBox IsChecked="{Binding IsSearchByCat}" Margin="0 0 10 0" VerticalAlignment="Center"
                          FontFamily="arial" FontSize="14">
                    Catégorie:
                </CheckBox>
                <ComboBox ItemsSource="{Binding CategorieList }" IsEnabled="{Binding IsSearchByCat}"
                          DisplayMemberPath="Designation" SelectedItem="{Binding SelectedCatSearch}" Width="100"
                          Height="20" />

            </StackPanel>
            <UniformGrid Grid.Row="3" Margin="5" Columns="3">

                <RadioButton Width="80" IsChecked="True" Cursor="Hand" Command="{Binding TousFTCmd}" Height="20"
                             Content="Tous" Style="{StaticResource {x:Type ToggleButton}}" />

                <RadioButton Width="80" Cursor="Hand" Command="{Binding ProdFTCmd}" Height="20" Content="Production"
                             Style="{StaticResource {x:Type ToggleButton}}" />

                <RadioButton Width="80" Cursor="Hand" Command="{Binding EchFTCmd}" Height="20" Content="Échantillon"
                             Style="{StaticResource {x:Type ToggleButton}}" />
            </UniformGrid>
            <ListView IsEnabled="{Binding DisplayDate}" FontSize="10" FontWeight="Bold" FontFamily="arial" Grid.Row="4"
                      SizeChanged="ListView_SizeChanged" ItemsSource="{Binding FicheTechniqueList}"
                      
                      SelectedItem="{Binding SelectedFicheTechnique}">
               
                <ListView.View>
                    
                    <GridView >
                        
                        <GridViewColumn Header="Réf" DisplayMemberBinding="{Binding Produits[0].Ref}" />
                        <GridViewColumn Header="Désignation" DisplayMemberBinding="{Binding Produits[0].Name}" />

                    </GridView>
                </ListView.View>

            </ListView>
            <StackPanel IsEnabled="{Binding IsAddEnabled}" Grid.Row="5" HorizontalAlignment="Center"
                        Orientation="Horizontal">

                <CustClass:GitHubButton FontSize="10" Cursor="Hand"
                                        Style="{StaticResource AddBtn}"
                                        Command="{Binding CmdValiderFicheTek}"
                                        Visibility="{Binding IsVerificateur,Converter={StaticResource BoolToVis}}">
                    Valider
                </CustClass:GitHubButton>
                <CustClass:GitHubButton FontSize="10" Cursor="Hand" Style="{StaticResource AddBtn}"
                                        Visibility="{Binding IsRedacteur,Converter={StaticResource BoolToVis}}"
                                        Command="{Binding CmdAddFicheTek}">
                    Ajouter
                </CustClass:GitHubButton>
                <CustClass:GitHubButton FontSize="10" Cursor="Hand" Style="{StaticResource ConfirmBtn}"
                                        Visibility="{Binding IsConfirm,Converter={StaticResource BoolToVis}}"
                                        Command="{Binding CmdConfirmer}">
                    Confirmer
                </CustClass:GitHubButton>
                <CustClass:GitHubButton FontSize="10" Cursor="Hand"
                                        Style="{StaticResource VersionBtn}"
                                        Visibility="{Binding IsPerm,Converter={StaticResource BoolToVis}}"
                                        Command="{Binding CmdAddNewVersion}">
                    Ajout Version
                </CustClass:GitHubButton>
                <CustClass:GitHubButton IsEnabled="{Binding IsTempo}"
                                        FontSize="10" Cursor="Hand" Style="{StaticResource EditBtn}"
                                        Command="{Binding CmdEditFicheTek}"
                                        Visibility="{Binding tempo,Converter={StaticResource BoolToVis}}">
                    Modifier
                </CustClass:GitHubButton>
                <CustClass:GitHubButton FontSize="10" Cursor="Hand" Style="{StaticResource RemoveBtn}"
                                        Command="{Binding CmdDeleteFicheTek}"
                                        Visibility="{Binding SuperUser,Converter={StaticResource BoolToVis}}">
                    Supprimer
                </CustClass:GitHubButton>
            </StackPanel>
        </Grid>

        <Grid Name="Frm"
              Grid.Column="1"
              Grid.ColumnSpan="2" 
                KeyDown="Frm_OnKeyDown"
              ClipToBounds="True">

            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition Width="40"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <local:FicheTekPart2 x:Name="Part2Tek" IsEnabled="{Binding EnableEditing}"
                                
                                 CustClass:perSizeBindingHelper.Active="True"
                                 CustClass:perSizeBindingHelper.BoundActualHeight="{Binding MainWinHeight, Mode=OneWayToSource}"
                                 CustClass:perSizeBindingHelper.BoundActualWidth="{Binding MainWinWidth, Mode=OneWayToSource}"
                                 Visibility="{Binding Part2Visible,Converter={StaticResource BoolToVis}}"
                                 Grid.RowSpan="2" />
            
            <local:FicheTekPart1 x:Name="Part1Tek" KeyboardNavigation.TabIndex="0" Grid.ColumnSpan="2"
                                 KeyboardNavigation.DirectionalNavigation="Local"
                                 Visibility="{Binding Part1Visible,Converter={StaticResource BoolToVis}}" />
                                 

        </Grid>

        <Grid Grid.Column="1" ColumnSpan="2" ZIndex="1" HorizontalAlignment="Right">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid HorizontalAlignment="Center"
                  Width="40" Name="ShortEnfView"
                  Visibility="{Binding Part2Visible,Converter={StaticResource BoolToVis}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <ListBox Grid.Row="1" HorizontalContentAlignment="Center"
 ItemsSource="{Binding SchCompList}" DisplayMemberPath="EnfNbrFil"  >
                  
                    <ListBox.ItemContainerStyle>
                        
                        <Style TargetType="{x:Type ListBoxItem}">

                            <Setter Property="Focusable" Value="False"></Setter>
                           
                            <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                            

                            <Setter Property="Border.BorderBrush" Value="Red" />
                            <Style.Triggers>
                                <DataTrigger Value="True">
                                    <DataTrigger.Binding>
                                        <MultiBinding>
                                            <MultiBinding.Converter>
                                                <Converter:EqualityConverter />
                                            </MultiBinding.Converter>
                                            <Binding Path="EnfNbrFil" NotifyOnTargetUpdated="True"
                                                                 NotifyOnSourceUpdated="True" />
                                            <Binding Path=" NbrFil" NotifyOnTargetUpdated="True"
                                                                 NotifyOnSourceUpdated="True" />
                                        </MultiBinding>
                                    </DataTrigger.Binding>
                                    <Setter Property="Border.BorderBrush" Value="Green" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </ListBox.ItemContainerStyle>
                </ListBox>

                <Border BorderThickness="1"  Grid.Row="0" BorderBrush="{Binding ElementName=DentAudit,Path=BorderBrush}">
                    
                    <StackPanel>

                        <TextBlock HorizontalAlignment="Center" >

                            <TextBlock.Text>
                                <MultiBinding StringFormat="{}{0}/{1}">
                                    <Binding Path="NewProd.EnfDent"  />
                                    <Binding Path="NewProd.Dent" />
                                </MultiBinding>
                            </TextBlock.Text>
                        </TextBlock>

                    </StackPanel>
                </Border>
                

            </Grid>
            <ToggleButton
                Height="30" Width="25"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                Cursor="Hand"
                IsChecked="False" BorderThickness="0"
                Style="{StaticResource tb_style}">
                <ToggleButton.Background>


                    <ImageBrush ImageSource="/Asset/back.png"
                                AlignmentY="Center"
                                AlignmentX="Center"
                                Stretch="Fill" />

                </ToggleButton.Background>

                ~2~// Toggle Button - Checked, Unchecked events //@2@

                <ToggleButton.Triggers>

                    <EventTrigger RoutedEvent="ToggleButton.Unchecked">
                        <BeginStoryboard>

                            <Storyboard x:Name="ShowQuickEnfilage">

                                <DoubleAnimation

                                    Storyboard.TargetName="ShortEnfView"
                                    Storyboard.TargetProperty="Opacity"
                                    BeginTime="0:0:0"

                                    From="0" To="1"
                                    Duration="0:0:0.3" />

                            </Storyboard>

                        </BeginStoryboard>
                        <BeginStoryboard>

                            <Storyboard x:Name="HideStackPanel">

                                <DoubleAnimation

                                    Storyboard.TargetName="nav_pnl"
                                    Storyboard.TargetProperty="Width"
                                    BeginTime="0:0:0"
                                    From="180" To="0"
                                    Duration="0:0:0.2" />

                            </Storyboard>

                        </BeginStoryboard>

                    </EventTrigger>

                    <EventTrigger RoutedEvent="ToggleButton.Checked">
                        <BeginStoryboard>

                            <Storyboard x:Name="HideQuickEnfilage">

                                <DoubleAnimation

                                    Storyboard.TargetName="ShortEnfView"
                                    Storyboard.TargetProperty="Opacity"
                                    BeginTime="0:0:0"
                                    From="1" To="0"
                                    Duration="0:0:0.2" />

                            </Storyboard>

                        </BeginStoryboard>


                        <BeginStoryboard>

                            <Storyboard x:Name="ShowStackPanel">

                                <DoubleAnimation

                                    Storyboard.TargetName="nav_pnl"
                                    Storyboard.TargetProperty="Width"
                                    BeginTime="0:0:0"
                                    From="0" To="180"
                                    Duration="0:0:0.3" />

                            </Storyboard>

                        </BeginStoryboard>

                    </EventTrigger>

                </ToggleButton.Triggers>
            </ToggleButton>
            <StackPanel Background="WhiteSmoke" HorizontalAlignment="Right" x:Name="nav_pnl" Width="0" Grid.Column="1"
                        KeyboardNavigation.DirectionalNavigation="None">
                <CustClass:GitHubButton Width="80" Style="{StaticResource PrintBtn}" Command="{Binding PrintCmd}"
                                        Panel.ZIndex="1">
                    Imprimer
                </CustClass:GitHubButton>
                <CheckBox
                    Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                    IsChecked="{Binding IsEnfilage}">
                    Enfilage
                </CheckBox>
                <GroupBox IsEnabled="{Binding IsEnfilage}" Header="Nbr Fils">
                    <StackPanel Orientation="Vertical">
                        <Grid>
                            <ListView x:Name="CompNbrAudit" ItemsSource="{Binding SchCompList}">
                               
                                <ListView.ItemContainerStyle>
                                    <Style TargetType="{x:Type ListViewItem}">
                                        <Setter Property="Border.BorderBrush" Value="Red" />
                                        <Style.Triggers>
                                            <DataTrigger Value="True">
                                                <DataTrigger.Binding>
                                                    <MultiBinding>
                                                        <MultiBinding.Converter>
                                                            <Converter:EqualityConverter />
                                                        </MultiBinding.Converter>
                                                        <Binding Path="EnfNbrFil" NotifyOnTargetUpdated="True"
                                                                 NotifyOnSourceUpdated="True" />
                                                        <Binding Path=" NbrFil" NotifyOnTargetUpdated="True"
                                                                 NotifyOnSourceUpdated="True" />
                                                    </MultiBinding>
                                                </DataTrigger.Binding>
                                                <Setter Property="Border.BorderBrush" Value="Green" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </ListView.ItemContainerStyle>
                                <ListView.View>
                                    <GridView>

                                        <GridViewColumn Header="Composant"
                                                        DisplayMemberBinding="{Binding GetComposant.Name,NotifyOnSourceUpdated=True,NotifyOnTargetUpdated=True}" />
                                        <GridViewColumn Header="Enfilage" x:Name="EnfK"
                                                        DisplayMemberBinding="{Binding EnfNbrFil,NotifyOnSourceUpdated=True,NotifyOnTargetUpdated=True}" />
                                        <GridViewColumn Header="Nbr Fils"
                                                        DisplayMemberBinding="{Binding NbrFil,NotifyOnSourceUpdated=True,NotifyOnTargetUpdated=True}" />

                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </Grid>
                    </StackPanel>

                </GroupBox>
                <GroupBox x:Name="DentAudit" IsEnabled="{Binding IsEnfilage}" Header="Nbr Dents">
                    <GroupBox.Style>
                        <Style TargetType="{x:Type GroupBox}">
                            <Setter Property="BorderBrush" Value="Red" />
                            <Style.Triggers>
                                <DataTrigger Value="True">
                                    <DataTrigger.Binding>
                                        <MultiBinding>
                                            <MultiBinding.Converter>
                                                <Converter:EqualityConverter />
                                            </MultiBinding.Converter>
                                            <Binding Path="NewProd.Dent" NotifyOnTargetUpdated="True"
                                                                 NotifyOnSourceUpdated="True" />
                                            <Binding Path="NewProd.EnfDent" NotifyOnTargetUpdated="True"
                                                                 NotifyOnSourceUpdated="True" />
                                        </MultiBinding>
                                    </DataTrigger.Binding>
                                    <Setter Property="BorderBrush" Value="Green" />

                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </GroupBox.Style>
                    <Grid >
                        
                        
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        
                        <TextBlock Grid.Column="1">Annoncé</TextBlock>
                        <TextBlock Grid.Column="1" Grid.Row="1" Text="{Binding NewProd.Dent,StringFormat=0;;#}" />

                        <TextBlock Grid.Column="0">Enfilage</TextBlock>
                        <TextBlock Grid.Row="1" Grid.Column="0" Text="{Binding NbrDent,StringFormat=0;;#}" />
                    </Grid>

                </GroupBox>
                <GroupBox IsEnabled="{Binding IsEnfilage}"
                          Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}" Header="Chaine">
                    <StackPanel>
                        <StackPanel Orientation="Vertical">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2.5*" />
                                    <ColumnDefinition Width="2*" />
                                </Grid.ColumnDefinitions>

                                <TextBlock FontSize="10" VerticalAlignment="Center" FontWeight="Bold">Chaine Séléctionnée</TextBlock>
                                <ComboBox IsEnabled="{Binding UseCreatedChaine}" DisplayMemberPath="Nom"
                                          ItemsSource="{Binding ChaineNameList}"
                                          SelectedItem="{Binding SelectedChaine}" Grid.Column="1" Height="20" />

                                <CheckBox Grid.Row="1" Command="{Binding CreateChaine}"
                                          IsChecked="{Binding IsCreateChaine}" FontSize="10">
                                    Création
                                </CheckBox>

                            </Grid>
                        </StackPanel>
                        <GroupBox IsEnabled="{Binding EnableChaineCreation}" FontSize="14" FontWeight="Bold"
                                  Header="Armure">
                            <StackPanel Orientation="Vertical">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock FontSize="12" FontWeight="Bold">designation:</TextBlock>
                                    <TextBox Grid.Column="1" Text="{Binding DesignationChaine}" />

                                    <TextBlock Grid.Row="1" FontSize="12" FontWeight="Bold" VerticalAlignment="Center">N° Colonne:</TextBlock>
                                    <xcd:IntegerUpDown Grid.Column="1" Grid.Row="1" Width="60" Minimum="1" Maximum="12"
                                                       Text="{Binding ChaineColumns,NotifyOnSourceUpdated=True,NotifyOnTargetUpdated=True,UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Grid.Row="2" FontSize="12" FontWeight="Bold" VerticalAlignment="Center">N° Lignes:</TextBlock>
                                    <xcd:IntegerUpDown Grid.Row="2" Grid.Column="1" Minimum="4" Maximum="96" Width="60"
                                                       Text="{Binding ChRowSum,NotifyOnSourceUpdated=True,NotifyOnTargetUpdated=True,UpdateSourceTrigger=PropertyChanged}" />
                                    <Button Grid.Row="3" Grid.ColumnSpan="2" Command="{Binding ValiderChaine}">Valider</Button>
                                </Grid>
                            </StackPanel>

                        </GroupBox>
                    </StackPanel>
                </GroupBox>


                <GroupBox Visibility="Collapsed" Header="Enfilage">
                    <ListView ItemsSource="{Binding ListComposant}" Width="100">
                        <ListView.View>

                            <GridView>
                                <GridViewColumn Header="Composant" DisplayMemberBinding="{Binding NameComposant}" />
                                <GridViewColumn Header="N° Fil">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBox Width="30" Text="{Binding NbrFil}" />


                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>

                                </GridViewColumn>
                                <GridViewColumn Header="Debut Fil">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>

                                            <TextBox Width="30" Text="{Binding DebutFil}" />


                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>

                                </GridViewColumn>
                                <GridViewColumn Header="Intermittent">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>

                                            <ComboBox>

                                                <ComboBoxItem Content="False" />
                                                <ComboBoxItem Content="True" IsSelected="{Binding Intermittent}" />
                                            </ComboBox>


                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>

                                </GridViewColumn>
                            </GridView>
                        </ListView.View>

                    </ListView>
                </GroupBox>
                <GroupBox Visibility="Collapsed" Header="Dent" Focusable="True">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock Text="N° Dents" />
                        <TextBox Width="30" Grid.Column="1" Height="20" Text="{Binding NumDents}" />
                        <TextBlock Grid.Row="1">Répétition</TextBlock>
                        <TextBox Grid.Row="1" Grid.Column="1" Width="30" Height="20" Text="{Binding Rep}" />
                        <TextBlock Grid.Row="2">Fil/Dent</TextBlock>
                        <TextBox Grid.Row="2" Grid.Column="1" Width="30" Height="20" Text="{Binding FilDent}" />
                    </Grid>

                </GroupBox>
                <Button 
                    
                    Width="100"
                    x:Name="RepBtn"
                    Command="{Binding CmdAddRep}"
                    Visibility="Collapsed">Ajouter Répétition</Button>
                <Button Width="80" Visibility="Collapsed">Appliquer</Button>
                <Button Width="80" Visibility="Collapsed">Vider Tous</Button>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="1" Background="Lavender">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*" />
                            <ColumnDefinition Width="0.1*" />
                            <ColumnDefinition Width="1.2*" />

                        </Grid.ColumnDefinitions>
                        <CustClass:GitHubButton Style="{StaticResource SaveBtn}"
                                                Visibility="{Binding SaveCancelBtn,Converter={StaticResource BoolToVis}}"
                                                Grid.Column="2"
                                                Cursor="Hand" Background="LightSteelBlue"
                                                Command="{Binding CmdSaveFicheTek}"
                                                VerticalAlignment="Top">
                            Enregistrer
                        </CustClass:GitHubButton>
                        <CustClass:GitHubButton Style="{StaticResource CancelBtn}"
                                                Visibility="{Binding SaveCancelBtn,Converter={StaticResource BoolToVis}}"
                                                Grid.Column="0"
                                                Cursor="Hand" Background="LightSteelBlue"
                                                Command="{Binding CmdCancelFicheTek}">
                            Annuler
                        </CustClass:GitHubButton>
                    </Grid>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Button Cursor="Hand" IsEnabled="{Binding IsPrevPage}" HorizontalAlignment="Right"
                                Grid.Column="0"
                                Command="{Binding PrevPage}" Width="auto" BorderBrush="Transparent"
                                Background="Transparent">
                            <Image Height="20" Source="../Asset/ArrowLeft.png" />
                        </Button>
                        <Border Grid.Column="1" BorderBrush="LightPink" Background="WhiteSmoke" CornerRadius="4"
                                BorderThickness="1" HorizontalAlignment="Center" VerticalAlignment="Center">

                            <TextBlock Text="{Binding PageNumber}" FontFamily="Arial" Width="20" />
                        </Border>
                        <Button IsEnabled="{Binding IsNextPage}" Cursor="Hand" HorizontalAlignment="Left"
                                Grid.Column="2"
                                Command="{Binding NextPage}" Width="auto" BorderBrush="Transparent"
                                Background="Transparent">
                            <Image Height="20" Source="../Asset/ArrowRight.png" />
                        </Button>
                    </Grid>
                </Grid>


            </StackPanel>
        </Grid>
    </Grid>
</view:MvxWpfView>