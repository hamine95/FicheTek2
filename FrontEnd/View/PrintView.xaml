<view:MvxWindow x:Class="FrontEnd.View.PrintView"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:view="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:glob="clr-namespace:System.Globalization;assembly=mscorlib"
                xmlns:Converter="clr-namespace:FrontEnd.Converter"
                xmlns:CustClass="clr-namespace:FrontEnd.CustomClass"
                xmlns:xtendToolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
                xmlns:Printing="clr-namespace:System.Drawing.Printing;assembly=System.Drawing"
                mc:Ignorable="d"
                 xmlns:converter="clr-namespace:FrontEnd.Converter"
                 xmlns:dSheetEnfilage="clr-namespace:DSheetEnfilage;assembly=DSheetEnfilage"
                 WindowState="Maximized"
                Loaded="PrintView_OnLoaded"
                Title="PrintView" Height="450" Width="800">
    <view:MvxWindow.Resources>
        <converter:TrameSizeConverter x:Key="TrameSizeConverter" />
        <Style x:Key="PrintBtn" TargetType="{x:Type CustClass:GitHubButton}">
            <Setter Property="UseLayoutRounding" Value="True" />
            <Setter Property="SnapsToDevicePixels" Value="True" />
            <Setter Property="Foreground" Value="#FFFFFF" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="Margin" Value="2" />
            <Setter Property="Height" Value="25" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type CustClass:GitHubButton}">
                        <ControlTemplate.Resources>
                            <Style TargetType="{x:Type Border}" x:Key="IN.BORDER">
                                <Setter Property="BorderBrush" Value="#24a0ed" />
                                <Setter Property="BorderThickness" Value="1 1 1 1" />
                                <Setter Property="Background" Value="#24a0ed" />
                                <Setter Property="CornerRadius" Value="5" />
                            </Style>
                            <Style TargetType="{x:Type StackPanel}" x:Key="IN.PANEL">
                                <Setter Property="VerticalAlignment" Value="Center" />
                                <Setter Property="Orientation" Value="Horizontal" />
                            </Style>
                            <Style TargetType="{x:Type Viewbox}" x:Key="IN.VBOX">
                                <Setter Property="VerticalAlignment" Value="Center" />
                                <Setter Property="Margin" Value="2 0 2 0" />
                                <Setter Property="Width" Value="16" />
                                <Setter Property="Height" Value="16" />
                            </Style>

                            <Style TargetType="{x:Type ContentPresenter}" x:Key="IN.CONTENT">
                                <Setter Property="VerticalAlignment" Value="Center" />
                                <Setter Property="Margin" Value="0 4 2 4" />
                            </Style>
                        </ControlTemplate.Resources>
                        <Border x:Name="border" Style="{StaticResource IN.BORDER}">
                            <StackPanel Style="{StaticResource IN.PANEL}">
                                <Viewbox Style="{StaticResource IN.VBOX}">
                                    <Image Width="24" Height="24" Source="/Asset/print.png" />
                                </Viewbox>
                                <ContentPresenter Style="{StaticResource IN.CONTENT}" />
                            </StackPanel>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="border" Property="Background" Value="#084469" />
                            </Trigger>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter TargetName="border" Property="Background" Value="#084469" />
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter TargetName="border" Property="Background" Value="#ADD8E6" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <converter:HeightConverter x:Key="HeightConvert" />
        <converter:BooleanToVisibilityValueConverter x:Key="BoolToVis" />
        
        <ResourceDictionary x:Key="dict1">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/DSheetEnfilage;component/legendStyle.xaml" ></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/DSheetEnfilage;component/ChaineStyle.xaml" ></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
         
    </view:MvxWindow.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1.2*"></ColumnDefinition>
            <ColumnDefinition Width="4*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid
            KeyDown="Grid_KeyDown"
            CustClass:perSizeBindingHelper.Active="True"
            CustClass:perSizeBindingHelper.BoundActualWidth="{Binding PanelWidth, Mode=OneWayToSource}"

            >
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <TextBlock 
                VerticalAlignment="Top" 
                FontFamily="cambria"
                Margin="5"
                FontSize="16" >Imprimant:</TextBlock>
            <ComboBox SelectionChanged="CmbPrinterSelection_OnSelectionChanged" Grid.Column="1" Margin="5" VerticalAlignment="Top" FontFamily="cambria" MaxHeight="25" Name="cmbPrinterSelection"  ItemsSource="{x:Static Printing:PrinterSettings.InstalledPrinters}" SelectedItem="{Binding SelectedPrint}"  />
           
            <CustClass:GitHubButton Cursor="Hand" Grid.Row="1" Grid.Column="2"   Style="{StaticResource PrintBtn}" Click="ButtonBase_OnClick"
            >Imprimer</CustClass:GitHubButton>
            <Grid
                Margin="5"
                VerticalAlignment="Center"
                
                Grid.ColumnSpan="2"
                Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                
                <CheckBox
                    VerticalContentAlignment="Center"
                    FontFamily="cambria"
                    FontSize="14"
                    Grid.Column="0"
                    x:Name="NumPageActivator"
                    >N° page</CheckBox>
                <xtendToolkit:IntegerUpDown
                    Grid.Column="1"
                    Width="50"
                    Minimum="1"
                    Maximum="2"
                    x:Name="NumPage"
                    Value="1"
                    IsEnabled="{Binding ElementName=NumPageActivator,Path=IsChecked}"
                    >
                    </xtendToolkit:IntegerUpDown>
            </Grid>
            <StackPanel
                Grid.Row="3"
                Grid.Column="0"
                Orientation="Horizontal"
                Visibility="Collapsed"
                x:Name="Xpanel"
                >
                <TextBlock>X:</TextBlock>
                <TextBlock 
                
                Text="{Binding TrameXposition}"
                ></TextBlock>
            </StackPanel>
            <StackPanel
                x:Name="Ypanel"
                Visibility="Collapsed"
                Grid.Row="3"
                Grid.Column="1"
                Orientation="Horizontal"
                >
                <TextBlock>Y:</TextBlock>
                <TextBlock 
                
                Text="{Binding TrameYposition}"
                ></TextBlock>
            </StackPanel>
        </Grid>
        <DocumentViewer Loaded="PreviewD_OnLoaded"

                        Grid.Column="1" ShowPageBorders="True" x:Name="DocViewer">
            <DocumentViewer.CommandBindings>
                <CommandBinding Command="ApplicationCommands.Print" Executed="Print_Executed" />
            </DocumentViewer.CommandBindings>
            <FixedDocument Loaded="FixedDocument_Loaded">
                <PageContent>
                    <FixedPage
                        Width="29.7cm"
                        Height="21cm"

                        VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                        <Grid

                            Width="29.7cm"
                            Height="21cm"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*" />
                                <RowDefinition Height="3*" />
                                <RowDefinition Height="0.5*" />
                                <RowDefinition Height="12*" />
                                <RowDefinition Height="0.5*" />
                                <RowDefinition Height="4*" />
                                <RowDefinition Height="2*" />
                                <RowDefinition Height="1*" />
                            </Grid.RowDefinitions>
                            <Grid Row="1" VerticalAlignment="Center" Height="2.5cm" HorizontalAlignment="Center"
                                  Width="26.4cm">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2*" />
                                    <ColumnDefinition Width="6*" />
                                    <ColumnDefinition Width="2*" />
                                </Grid.ColumnDefinitions>
                                <Border BorderBrush="Black" BorderThickness="1">
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <Border Height="80" CornerRadius="35" Width="170" BorderBrush="Black"
                                                BorderThickness="2">
                                            <Image Height="70" Width="100" Source="../Asset/logoTTBM.png" />
                                        </Border>

                                    </StackPanel>
                                </Border>
                                <Border BorderBrush="Black" BorderThickness="1" Grid.Column="1">
                                    <StackPanel>
                                        <TextBlock HorizontalAlignment="Center" Margin="0 0 0 20" FontSize="35"
                                                   FontWeight="Bold"
                                                   FontFamily="Monotype Corsiva">
                                            Fiche Technique
                                        </TextBlock>
                                        <Separator BorderBrush="Black" BorderThickness="1" Width="200" />
                                        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
                                            <TextBlock FontSize="18" HorizontalAlignment="Center"
                                                       VerticalAlignment="Center"
                                                       FontFamily="Imprint MT Shadow">
                                                N° Article
                                            </TextBlock>
                                            <TextBlock Width="50" FontSize="18"
                                                       VerticalAlignment="Center"

                                                       FontFamily="Imprint MT Shadow" HorizontalAlignment="Right"
                                                       TextAlignment="Center"
                                                       Text="{Binding NewProd.NumArticle,StringFormat=0;;#}" />
                                        </StackPanel>
                                    </StackPanel>
                                </Border>

                                <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="1*" />
                                            <ColumnDefinition Width="4*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <TextBlock FontSize="14" FontFamily="Arial" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center">
                                            Vers
                                        </TextBlock>
                                        <TextBlock FontSize="14" HorizontalAlignment="Center" Grid.Column="1">:</TextBlock>

                                        <TextBlock
                                            FontSize="14" FontFamily="Arial" Grid.Column="2"
                                            Text="{Binding NewProd.Version,StringFormat={}{00:00}}"
                                            VerticalAlignment="Center"
                                            HorizontalAlignment="Center" />
                                        <TextBlock FontSize="14" FontFamily="Arial" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   Grid.Row="1">
                                            Réf
                                        </TextBlock>
                                        <TextBlock FontSize="14" Grid.Row="1" HorizontalAlignment="Center"
                                                   Grid.Column="1">
                                            :
                                        </TextBlock>

                                        <TextBlock HorizontalAlignment="Center"

                                                   VerticalAlignment="Center" FontFamily="Arial"
                                                   FontSize="14" Grid.Row="1" Grid.Column="2"
                                                   Text="{Binding NewProd.Ref}" />
                                        <TextBlock FontSize="14" FontFamily="Arial" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   Grid.Row="2">
                                            Page
                                        </TextBlock>
                                        <TextBlock FontSize="14" Grid.Row="2" HorizontalAlignment="Center"
                                                   Grid.Column="1">
                                            :
                                        </TextBlock>
                                        <TextBlock FontSize="14" FontFamily="Arial" Grid.Row="2" Grid.Column="2"
                                                   VerticalAlignment="Center"
                                                   HorizontalAlignment="Center">
                                            1/2
                                        </TextBlock>
                                    </Grid>
                                </Border>

                            </Grid>

                            <Grid Grid.Row="3" Height="9.9cm" VerticalAlignment="Center" HorizontalAlignment="Center"
                                  Width="27.1cm">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="2.5*" />
                                    <RowDefinition Height="0.5*" />
                                    <RowDefinition Height="7*" />
                                    <RowDefinition Height="1.5*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="6*" />
                                    <ColumnDefinition Width="1*" />
                                </Grid.ColumnDefinitions>
                                <Grid Grid.ColumnSpan="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*" />
                                        <RowDefinition Height="2*" />
                                    </Grid.RowDefinitions>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="6.5*" />
                                                <ColumnDefinition Width="5.5*" />
                                            <ColumnDefinition Width="2*" />
                                        </Grid.ColumnDefinitions>
                                        <Border BorderBrush="Black"  BorderThickness="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="1*" />
                                                    <ColumnDefinition Width="2*" />

                                                </Grid.ColumnDefinitions>
                                                <TextBlock FontWeight="Bold" FontFamily="Arial" FontSize="14"
                                                           VerticalAlignment="Center"
                                                           HorizontalAlignment="Center">
                                                    Désignation TTBM :
                                                </TextBlock>

                                                <TextBlock Grid.Column="1" FontFamily="Arial"
                                                           FontWeight="Bold" FontSize="12"

                                                           VerticalAlignment="Center" HorizontalAlignment="Center"
                                                           Text="{Binding NewProd.Name}" />
                                            </Grid>
                                        </Border>

                                        <Border
                        Grid.Column="1"
                        Visibility="{Binding IsFicheNormal,Converter={StaticResource BoolToVis}}"
                        BorderBrush="Black" BorderThickness="0 1 0 1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="1*" />
                                                    <ColumnDefinition Width="3*" />
                                                </Grid.ColumnDefinitions>


                                                <TextBlock Grid.Column="0" FontSize="14" FontFamily="Arial" FontWeight="Bold"

                                       VerticalAlignment="Center" HorizontalAlignment="Center">
                                Client :
                                                </TextBlock>

                                                <StackPanel
                                Grid.Column="1"
                                VerticalAlignment="Center">
                                                    <TextBlock Cursor="Hand"
                                           
                                           FontWeight="Bold" FontFamily="Arial" FontSize="12"
                                           HorizontalAlignment="Center" VerticalAlignment="Center"
                                           Text="{Binding NewProd.Client.Name}" />
                                                </StackPanel>

                                            </Grid>

                                        </Border>
                                        <Border
                        Visibility="{Binding IsFicheEHC,Converter={StaticResource BoolToVis}}"
                        ClipToBounds="False" Grid.ColumnSpan="2"
                        Grid.Column="1" BorderBrush="Black" BorderThickness="0 1 1 1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="2*" />
                                                    <ColumnDefinition Width="3*" />
                                                    <ColumnDefinition Width="3*" />
                                                </Grid.ColumnDefinitions>


                                                <TextBlock


                                Grid.Column="0" FontSize="14" FontFamily="Arial" FontWeight="Bold"
                                VerticalAlignment="Center" HorizontalAlignment="Center">
                                Désignation E.H.C :
                                                </TextBlock>

                                                <TextBlock Cursor="Hand"
                                       Grid.Column="1"
                                       Grid.ColumnSpan="2"
                                       
                                       FontWeight="Bold" FontFamily="Arial" FontSize="14"
                                       HorizontalAlignment="Center" VerticalAlignment="Center"
                                       Text="{Binding NewProd.Name2}" />

                                            </Grid>

                                        </Border>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3*" />
                                            <ColumnDefinition Width="3*" />
                                            <ColumnDefinition Width="3*" />
                                            <ColumnDefinition Width="1*" />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Border BorderBrush="Black" BorderThickness="1 0 1 1">

                                            <StackPanel Orientation="Horizontal">
                                                <TextBlock FontSize="16" Margin="5 0 0 0" FontFamily="arial"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center">
                                                    Dimension :
                                                </TextBlock>
                                                <TextBlock
                                                    Width="30" Foreground="Black" FontSize="20"
                                                    FontWeight="Bold"
                                                    FontFamily="arial"
                                                    VerticalAlignment="Center"
                                                    Text="{Binding NewProd.Largeur,StringFormat=0;;#}" />
                                                <TextBlock FontSize="16" FontFamily="arial"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center">
                                                    mm
                                                </TextBlock>
                                            </StackPanel>
                                        </Border>
                                        <Border Grid.Column="1" BorderBrush="Black" BorderThickness="0 0 1 1">
                                            <DockPanel>
                                                <TextBlock FontSize="16" Margin="5 0 0 0 " FontFamily="arial"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center">
                                                    Duitage :
                                                </TextBlock>
                                                <Grid>

                                                    <TextBlock Cursor="Hand" FontSize="16" FontFamily="arial"

                                                               VerticalAlignment="Center"
                                                               HorizontalAlignment="Center"
                                                               Text="{Binding NewProd.DuitageID.Duitage}" />
                                                </Grid>
                                            </DockPanel>
                                        </Border>
                                        <Border Grid.Column="2" BorderBrush="Black" BorderThickness="0 0 1 1">
                                            <DockPanel>
                                                <TextBlock FontFamily="arial" Margin="5 0 0 0 " FontSize="16"
                                                           VerticalAlignment="Center"
                                                           HorizontalAlignment="Center">
                                                    Dent :
                                                </TextBlock>
                                                <Grid>

                                                    <TextBlock
                                                        FontFamily="arial" FontSize="16" HorizontalAlignment="Center"

                                                        VerticalAlignment="Center"
                                                        Text="{Binding NewProd.Dent,StringFormat=0;;#}" />
                                                </Grid>
                                            </DockPanel>
                                        </Border>

                                        <Border Grid.Row="1" Grid.Column="0" BorderBrush="Black"
                                                BorderThickness="1 0 1 1">
                                            <DockPanel>
                                                <TextBlock FontSize="16" Margin="5 0 0 0 " FontFamily="arial"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center">
                                                    Vitesse ML/MIN :
                                                </TextBlock>
                                                <Grid>
                                                    <TextBlock FontSize="16" FontFamily="arial"
                                                               HorizontalAlignment="Center"
                                                               VerticalAlignment="Center" Text="{Binding Vitesse}" />
                                                    <TextBlock FontSize="16" FontFamily="arial"
                                                               HorizontalAlignment="Center"
                                                               VerticalAlignment="Center"
                                                               Text="{Binding NewProd.DuitageID.Vitesse}" />
                                                </Grid>
                                            </DockPanel>
                                        </Border>

                                        <Border Grid.Row="1" Grid.Column="1" BorderBrush="Black"
                                                BorderThickness="0 0 1 1">
                                            <DockPanel>
                                                <TextBlock FontSize="16" Margin="5 0 0 0 " FontFamily="arial"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center">
                                                    Peigne :
                                                </TextBlock>
                                                <Grid HorizontalAlignment="Center">

                                                    <TextBlock

                                                        FontSize="16" Width="50" FontFamily="arial"
                                                        HorizontalAlignment="Right"
                                                        TextAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding NewProd.PeigneObj.Nombre,StringFormat=0.0;;#}" />
                                                </Grid>
                                                <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Left"
                                                           VerticalAlignment="Center">
                                                    D/CM
                                                </TextBlock>
                                            </DockPanel>
                                        </Border>

                                        <Border Grid.Row="1" Grid.Column="2" BorderBrush="Black"
                                                BorderThickness="0 0 1 1">
                                            <DockPanel>
                                                <TextBlock FontSize="16" Margin="5 0 0 0 " FontFamily="arial"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center">
                                                    Machine :
                                                </TextBlock>
                                                <Grid>

                                                    <TextBlock FontSize="16" TextSearch.TextPath="Name"

                                                               FontFamily="arial" HorizontalAlignment="Center"
                                                               VerticalAlignment="Center"
                                                               Text="{Binding NewProd.DuitageID.Machine.Name}" />
                                                </Grid>
                                            </DockPanel>
                                        </Border>


                                    </Grid>

                                </Grid>

                                <Border

                Visibility="{Binding IsFicheNormal,Converter={StaticResource BoolToVis}}"
                Grid.Row="0" Grid.Column="1" Grid.RowSpan="4" BorderThickness="1" BorderBrush="Black">
                                    <Grid ZIndex="8">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="0.6*" />
                                            <RowDefinition Height="8*" />
                                        </Grid.RowDefinitions>
                                        <Border BorderBrush="Black" BorderThickness="0 0 0 1">
                                            <Grid>
                                                <TextBlock FontFamily="Monotype Corsiva" FontSize="18" FontWeight="Bold"
                                       VerticalAlignment="Center" HorizontalAlignment="Center">
                                Echantillon
                                                </TextBlock>

                                            </Grid>

                                        </Border>
                                    </Grid>
                                </Border>
                                <Border

                Visibility="{Binding IsFicheEHC,Converter={StaticResource BoolToVis}}"
                Margin="0 30 0 0" Grid.Row="0" Grid.Column="1" Grid.RowSpan="4" BorderThickness="1" BorderBrush="Black">
                                    <Grid ZIndex="8">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="0.6*" />
                                            <RowDefinition Height="8*" />
                                        </Grid.RowDefinitions>
                                        <Border BorderBrush="Black" BorderThickness="0 0 0 1">
                                            <Grid>
                                                <TextBlock FontFamily="Monotype Corsiva" FontSize="18" FontWeight="Bold"
                                       VerticalAlignment="Center" HorizontalAlignment="Center">
                                Echantillon
                                                </TextBlock>

                                            </Grid>

                                        </Border>
                                    </Grid>
                                </Border>
                                <Grid Grid.Row="2" Grid.RowSpan="2" HorizontalAlignment="Stretch">

                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="0.1*" />
                                        <ColumnDefinition Width="2*" />
                                        <ColumnDefinition Width="6*" />
                                        <ColumnDefinition Width="1.5*" />
                                        <ColumnDefinition Width="1.5*" />
                                        <ColumnDefinition Width="1.5*" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="2*" />
                                        <ColumnDefinition Width="3*" />
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1 1 1 1">
                                        <TextBlock FontSize="16" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center">
                                            Composition
                                        </TextBlock>
                                    </Border>

                                    <Border Grid.Column="2" BorderBrush="Black" BorderThickness="0 1 1 1">
                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center">
                                            <Run FontSize="16">Matiere 1</Run>
                                            <Run FontSize="16" Typography.Variants="Superscript">ere</Run>
                                        </TextBlock>
                                    </Border>
                                    <Border Grid.Column="3" BorderBrush="Black" BorderThickness="0 1 1 1">
                                        <TextBlock FontSize="16" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center">
                                            Nbr fils
                                        </TextBlock>
                                    </Border>
                                    <Border Grid.Column="4" BorderBrush="Black" BorderThickness="0 1 1 1">
                                        <TextBlock FontSize="16" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center">
                                            Torsion
                                        </TextBlock>
                                    </Border>
                                    <Border Grid.Column="5" BorderBrush="Black" BorderThickness="0 1 1 1">
                                        <TextBlock FontSize="16" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center">
                                            Enfilage
                                        </TextBlock>
                                    </Border>
                                    <Border Grid.Column="6" BorderBrush="Black" BorderThickness="0 1 1 1">
                                        <TextBlock FontSize="16" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center">
                                            Emb%
                                        </TextBlock>
                                    </Border>
                                    <Border Grid.Column="7" BorderBrush="Black" BorderThickness="0 1 1 1">
                                        <TextBlock FontSize="16" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center">
                                            KG/1000ml
                                        </TextBlock>
                                    </Border>
                                    <Border Grid.Column="8" BorderBrush="Black" BorderThickness="0 1 0 1">
                                        <TextBlock FontSize="16" HorizontalAlignment="Center"
                                                   VerticalAlignment="Center">
                                            Observation
                                        </TextBlock>
                                    </Border>
                                    <Border Grid.Row="1" BorderBrush="Black" Grid.Column="1" BorderThickness="1 0 1 1">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*" />
                                                <ColumnDefinition Width="1*" />
                                            </Grid.ColumnDefinitions>

                                            <TextBlock
                                                TextAlignment="Center"
                                                HorizontalAlignment="Stretch" VerticalAlignment="Center"
                                                FontSize="16"
                                                Text="{Binding Comp1.GetComposant.Name}" />

                                            <TextBlock TextAlignment="Center"


                                                       HorizontalAlignment="Stretch" Grid.Column="1"
                                                       FontSize="16"
                                                       VerticalAlignment="Center"
                                                       Text="{Binding Comp1.Num,StringFormat=0;;#}" />
                                        </Grid>
                                    </Border>
                                    <Border Grid.Row="2" Grid.Column="1" BorderBrush="Black" BorderThickness="1 0 1 1">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*" />
                                                <ColumnDefinition Width="1*" />
                                            </Grid.ColumnDefinitions>

                                            <TextBlock
                                                TextAlignment="Center"
                                                HorizontalAlignment="Stretch" VerticalAlignment="Center"
                                                FontSize="16"
                                                Text="{Binding Comp2.GetComposant.Name}" />

                                            <TextBlock
                                                TextAlignment="Center"

                                                HorizontalAlignment="Stretch" Grid.Column="1"
                                                VerticalAlignment="Center" Text="{Binding Comp2.Num,StringFormat=0;;#}"
                                                FontSize="16" />
                                        </Grid>
                                    </Border>
                                    <Border Grid.Row="3" Grid.Column="1" BorderBrush="Black" BorderThickness="1 0 1 1">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*" />
                                                <ColumnDefinition Width="1*" />
                                            </Grid.ColumnDefinitions>

                                            <TextBlock
