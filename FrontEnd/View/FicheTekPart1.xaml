<UserControl x:Class="FrontEnd.View.FicheTekPart1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Converter="clr-namespace:FrontEnd.Converter"
             xmlns:xcd="http://schemas.xceed.com/wpf/xaml/toolkit"
             mc:Ignorable="d"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:CustClass="clr-namespace:FrontEnd.CustomClass"
             d:DesignHeight="750" d:DesignWidth="800">
    <UserControl.Resources>
        <sys:Boolean x:Key="BoolParam">true</sys:Boolean>
        <Storyboard x:Key="expandStoryboard">
            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.ScaleX"
                             To="2" Duration="0:0:0.2" />

            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.ScaleY"
                             To="2" Duration="0:0:0.2" />
        </Storyboard>
        <!-- This storyboard will make the image revert to its original size -->
        <Storyboard x:Key="shrinkStoryboard">
            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.ScaleX"
                             To="1" Duration="0:0:0.2" />
            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.ScaleY"
                             To="1" Duration="0:0:0.2" />
        </Storyboard>
        <Converter:InverseBooleanConverter x:Key="InverseBool" />
        <Converter:BooleanToVisibilityValueConverter x:Key="BoolToVis" />
        <Style x:Key="ImageLBtn" TargetType="{x:Type CustClass:GitHubButton}">
            <Setter Property="UseLayoutRounding" Value="True" />
            <Setter Property="SnapsToDevicePixels" Value="True" />
            <Setter Property="Foreground" Value="#FFFFFF" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="Margin" Value="2" />
            <Setter Property="Height" Value="25" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type CustClass:GitHubButton}">
                        <ControlTemplate.Resources>
                            <Style TargetType="{x:Type Border}" x:Key="IN.BORDER">
                                <Setter Property="BorderBrush" Value="#24a0ed" />
                                <Setter Property="BorderThickness" Value="1 1 1 1" />
                                <Setter Property="Background" Value="#24a0ed" />
                                <Setter Property="CornerRadius" Value="5" />
                            </Style>
                            <Style TargetType="{x:Type StackPanel}" x:Key="IN.PANEL">
                                <Setter Property="VerticalAlignment" Value="Center" />
                                <Setter Property="Orientation" Value="Horizontal" />
                            </Style>
                            <Style TargetType="{x:Type Viewbox}" x:Key="IN.VBOX">
                                <Setter Property="VerticalAlignment" Value="Center" />
                                <Setter Property="Margin" Value="2 0 2 0" />
                                <Setter Property="Width" Value="16" />
                                <Setter Property="Height" Value="16" />
                            </Style>

                        </ControlTemplate.Resources>
                        <Border x:Name="border" Style="{StaticResource IN.BORDER}">
                            <StackPanel Style="{StaticResource IN.PANEL}">
                                <Viewbox Style="{StaticResource IN.VBOX}">
                                    <Image Source="/Asset/imageLoad.png" Width="24" Height="24" />
                                </Viewbox>

                            </StackPanel>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="border" Property="Background" Value="#084469" />
                            </Trigger>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter TargetName="border" Property="Background" Value="#084469" />
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter TargetName="border" Property="Background" Value="#ADD8E6" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Resources>
    <Grid >
        <Grid.RowDefinitions>

            <RowDefinition Height="2*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="12*" />
            <RowDefinition Height="2*" />
            <RowDefinition Height="3.5*" />
            <RowDefinition Height="2*" />
        </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="2*" />
                <ColumnDefinition Width="6*" />
                <ColumnDefinition Width="2*" />
            </Grid.ColumnDefinitions>
            <Border BorderBrush="Black" BorderThickness="1">
                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                    <Border CornerRadius="10" Width="80" BorderBrush="Black" BorderThickness="2">
                        <Image Height="50" Width="70" Source="../Asset/logoTTBM.png" />
                    </Border>

                </StackPanel>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="1">
                <StackPanel>
                    <TextBlock HorizontalAlignment="Center" FontSize="28" FontWeight="Bold"
                               FontFamily="Monotype Corsiva">
                        Fiche Technique
                    </TextBlock>
                    <Separator BorderBrush="Black" BorderThickness="1" Width="200" />
                    <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
                        <TextBlock FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center"
                                   FontFamily="Imprint MT Shadow">
                            N° Article
                        </TextBlock>
                        <TextBox Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                 PreviewTextInput="Comp1NbrFilAccept" Width="50" FontSize="14"
                                 HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                                 BorderBrush="Transparent" FontFamily="Imprint MT Shadow" HorizontalAlignment="Stretch"
                                 Text="{Binding NewProd.NumArticle,StringFormat=0;;#}" />
                        <TextBlock PreviewTextInput="Comp1NbrFilAccept" Width="50" FontSize="14"
                                   VerticalAlignment="Center"
                                   Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                   FontFamily="Imprint MT Shadow" HorizontalAlignment="Right" TextAlignment="Center"
                                   Text="{Binding NewProd.NumArticle,StringFormat=0;;#}" />
                    </StackPanel>
                </StackPanel>
            </Border>

            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*" />
                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="4*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <TextBlock FontSize="14" FontFamily="Arial" HorizontalAlignment="Center" VerticalAlignment="Center">Vers</TextBlock>
                    <TextBlock FontSize="14" HorizontalAlignment="Center" Grid.Column="1">:</TextBlock>
                    <xcd:IntegerUpDown Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                       IsEnabled="{Binding IsProduction}"
                                       Minimum="{Binding MinVers}"

                                       FontSize="14" FontFamily="Arial" BorderBrush="Transparent" Grid.Column="2"
                                       Text="{Binding NewProd.Version,StringFormat={}{00:00}}"
                                       VerticalContentAlignment="Center"
                                       HorizontalAlignment="Center" />
                    <ComboBox Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}" FontSize="14"
                              FontFamily="Arial" Grid.Column="2"
                              SelectedItem="{Binding SelectedVersion}" VerticalAlignment="Center"
                              ItemsSource="{Binding ListVersion}"
                              DisplayMemberPath="Version"
                              HorizontalAlignment="Center" />
                    <TextBlock FontSize="14" FontFamily="Arial" HorizontalAlignment="Center" VerticalAlignment="Center"
                               Grid.Row="1">
                        Réf
                    </TextBlock>
                    <TextBlock FontSize="14" Grid.Row="1" HorizontalAlignment="Center" Grid.Column="1">:</TextBlock>
                    <TextBox HorizontalAlignment="Stretch" CharacterCasing="Upper" HorizontalContentAlignment="Center"
                             Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                             VerticalContentAlignment="Center" FontFamily="Arial" BorderBrush="Transparent"
                             FontSize="14" Grid.Row="1" Grid.Column="2" Text="{Binding NewProd.Ref}" />
                    <TextBlock HorizontalAlignment="Center"
                               Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                               VerticalAlignment="Center" FontFamily="Arial"
                               FontSize="14" Grid.Row="1" Grid.Column="2" Text="{Binding NewProd.Ref}" />
                    <TextBlock FontSize="14" FontFamily="Arial" HorizontalAlignment="Center" VerticalAlignment="Center"
                               Grid.Row="2">
                        Page
                    </TextBlock>
                    <TextBlock FontSize="14" Grid.Row="2" HorizontalAlignment="Center" Grid.Column="1">:</TextBlock>
                    <TextBlock FontSize="14" FontFamily="Arial" Grid.Row="2" Grid.Column="2"
                               HorizontalAlignment="Center">
                        1/2
                    </TextBlock>
                </Grid>
            </Border>

        </Grid>

        <Grid Grid.Row="1" Height="25" IsEnabled="{Binding EnableEditing}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.8*" />
                <ColumnDefinition Width="2*" />
                <ColumnDefinition Width="0.5*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="2*" />
                <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <TextBlock
                FontSize="16" FontWeight="Bold"
                HorizontalAlignment="Center"
                VerticalAlignment="Center">
                Catégorie:
            </TextBlock>
            <ComboBox Grid.Column="1"
                      DisplayMemberPath="Designation"
                      Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                      SelectedItem="{Binding SelectedCategorie}"
                      ItemsSource="{Binding CategorieList}" />
            <TextBlock Grid.Column="1" FontSize="16" VerticalAlignment="Center"
                       Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                       Text="{Binding CategorieName}" />

            <TextBlock
                Grid.Column="3"
                FontSize="16" FontWeight="Bold"
                HorizontalAlignment="Center"
                VerticalAlignment="Center">
                Fusionner:
            </TextBlock>
            <ComboBox Grid.Column="4"
                      DisplayMemberPath="Name"
                      
                      Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                      SelectedItem="{Binding FicheTechniqueFusionner}"
                      ItemsSource="{Binding FicheTechniqueFusionList}" />
        </Grid>
        <Grid ZIndex="1" Grid.Row="2">
            <Grid.RowDefinitions>
                <RowDefinition Height="2.5*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="6*" />
                <RowDefinition Height="2*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="6*" />
                <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Grid Grid.ColumnSpan="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*" />
                    <RowDefinition Height="2*" />
                </Grid.RowDefinitions>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="6.5*" />
                        <ColumnDefinition Width="5.5*" />
                        <ColumnDefinition Width="2*" />
                    </Grid.ColumnDefinitions>
                    <Border BorderBrush="Black" BorderThickness="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="2*" />

                            </Grid.ColumnDefinitions>
                            <TextBlock FontWeight="Bold" FontFamily="Arial" FontSize="14" VerticalAlignment="Center"
                                       HorizontalAlignment="Center">
                                Désignation TTBM :
                            </TextBlock>
                            <TextBox CharacterCasing="Upper" Grid.Column="1" FontFamily="Arial"
                                     BorderBrush="Transparent" FontWeight="Bold" FontSize="10"
                                     Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                     VerticalContentAlignment="Center" HorizontalContentAlignment="Center"
                                     Text="{Binding NewProd.Name}" />
                            <TextBlock Grid.Column="1" FontFamily="Arial"
                                       FontWeight="Bold" FontSize="10"
                                       Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                       VerticalAlignment="Center" HorizontalAlignment="Center"
                                       Text="{Binding NewProd.Name}" />
                        </Grid>
                    </Border>

                    <Border
                        Grid.Column="1"
                        Visibility="{Binding IsFicheEHC,Converter={StaticResource BoolToVis},ConverterParameter={StaticResource BoolParam}}"
                        BorderBrush="Black" BorderThickness="0 1 0 1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="3*" />
                            </Grid.ColumnDefinitions>


                            <TextBlock Grid.Column="0" FontSize="14" FontFamily="Arial" FontWeight="Bold"

                                       VerticalAlignment="Center" HorizontalAlignment="Center">
                                Client :
                            </TextBlock>

                            <StackPanel
                                Grid.Column="1"
                                VerticalAlignment="Center">
                                <ComboBox
                                    Cursor="Hand"
                                    DisplayMemberPath="Name" BorderBrush="Transparent"
                                    FontWeight="Bold" FontFamily="Arial" FontSize="12"
                                    Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                    HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                                    SelectedItem="{Binding NewProd.Client}" ItemsSource="{Binding ListClient}" />
                                <TextBlock Cursor="Hand"
                                           Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                           FontWeight="Bold" FontFamily="Arial" FontSize="12"
                                           HorizontalAlignment="Center" VerticalAlignment="Center"
                                           Text="{Binding NewProd.Client.Name}" />
                            </StackPanel>

                        </Grid>

                    </Border>
                    <Border
                        Visibility="{Binding IsFicheEHC,Converter={StaticResource BoolToVis}}"
                        ClipToBounds="False" Grid.ColumnSpan="2"
                        Grid.Column="1" BorderBrush="Black" BorderThickness="0 1 1 1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2*" />
                                <ColumnDefinition Width="3*" />
                                <ColumnDefinition Width="3*" />
                            </Grid.ColumnDefinitions>


                            <TextBlock


                                Grid.Column="0" FontSize="14" FontFamily="Arial" FontWeight="Bold"
                                VerticalAlignment="Center" HorizontalAlignment="Center">
                                Désignation E.H.C :
                            </TextBlock>

                            <TextBox
                                Grid.Column="1"
                                Grid.ColumnSpan="2"
                                CharacterCasing="Upper"
                                BorderBrush="Transparent"
                                FontWeight="Bold" FontFamily="Arial" FontSize="12"
                                Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                                Text="{Binding NewProd.Name2}" />
                            <TextBlock Cursor="Hand"
                                       Grid.Column="1"
                                       Grid.ColumnSpan="2"
                                       Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                       FontWeight="Bold" FontFamily="Arial" FontSize="14"
                                       HorizontalAlignment="Center" VerticalAlignment="Center"
                                       Text="{Binding NewProd.Name2}" />

                        </Grid>

                    </Border>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="3*" />
                        <ColumnDefinition Width="3*" />
                        <ColumnDefinition Width="3*" />
                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Border BorderBrush="Black" BorderThickness="1 0 1 1">

                        <StackPanel Orientation="Horizontal">
                            <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                Dimension :
                            </TextBlock>
                            <TextBox PreviewTextInput="Comp1NbrFilAccept" DataObject.Pasting="TextBox_Pasting"
                                     BorderBrush="Transparent" Width="30" Foreground="Red" FontSize="16"
                                     FontFamily="arial" VerticalContentAlignment="Center"
                                     HorizontalContentAlignment="Center"
                                     Text="{Binding NewProd.Largeur,StringFormat=0;;#}" />
                            <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                       Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                       VerticalAlignment="Center">
                                X
                            </TextBlock>
                            <TextBox PreviewTextInput="Comp1NbrFilAccept" DataObject.Pasting="TextBox_Pasting"
                                     Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                     BorderBrush="Transparent" Width="30" Foreground="Red" FontSize="16"
                                     FontFamily="arial" VerticalContentAlignment="Center"
                                     HorizontalContentAlignment="Center"
                                     Text="{Binding NewProd.Epaisseur,StringFormat=0;;#}" />
                            <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                mm
                            </TextBlock>
                        </StackPanel>
                    </Border>
                    <Border Grid.Column="1" BorderBrush="Black" BorderThickness="0 0 1 1">
                        <Grid>
                            <DockPanel
                                Visibility="{Binding IsFicheTressage,Converter={StaticResource BoolToVis},ConverterParameter={StaticResource BoolParam}}"
                                >
                                <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                Duitage :
                                </TextBlock>
                                <Grid>
                                    <ComboBox Cursor="Hand" FontSize="16" FontFamily="arial"
                                          IsTextSearchEnabled="True"
                                          TextSearch.TextPath="Duitage"
                                          Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                          HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                                          HorizontalAlignment="Stretch" ItemsSource="{Binding DuitageList}"
                                          DisplayMemberPath="Duitage" SelectedItem="{Binding NewProd.DuitageID}" />
                                    <TextBlock Cursor="Hand" FontSize="16" FontFamily="arial"
                                           Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                           VerticalAlignment="Center"
                                           HorizontalAlignment="Center"
                                           Text="{Binding NewProd.DuitageID.Duitage}" />
                                </Grid>
                            </DockPanel>
                            <DockPanel 
                                Visibility="{Binding IsFicheTressage,Converter={StaticResource BoolToVis}}"
                                >
                                <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                Machine :
                                </TextBlock>
                                <Grid>
                                    <ComboBox FontSize="16" IsTextSearchEnabled="True" TextSearch.TextPath="Num"


                                          FontFamily="arial" HorizontalContentAlignment="Stretch"
                                          VerticalContentAlignment="Center" Cursor="Hand" DisplayMemberPath="Name"
                                          Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                          HorizontalAlignment="Stretch"

                                          ScrollViewer.VerticalScrollBarVisibility="Visible"
                                          SelectedItem="{Binding SelectedMachine}" ItemsSource="{Binding MachineList}" />
                                    <TextBlock FontSize="16"
                                           Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                           FontFamily="arial" HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Text="{Binding NewProd.DuitageID.Machine.Name}" />
                                </Grid>
                            </DockPanel>
                        </Grid>
                       
                        
                    </Border>
                    <Border Grid.Column="2" BorderBrush="Black" BorderThickness="0 0 1 1">
                        <DockPanel>
                            <TextBlock FontFamily="arial" FontSize="16" VerticalAlignment="Center"
                                       HorizontalAlignment="Center">
                                Dent :
                            </TextBlock>
                            <Grid>
                                <TextBox PreviewTextInput="Comp1NbrFilAccept" DataObject.Pasting="TextBox_Pasting"
                                         FontFamily="arial" FontSize="16" HorizontalContentAlignment="Center"
                                         Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                         VerticalContentAlignment="Center" BorderBrush="Transparent"
                                         Text="{Binding NewProd.Dent,StringFormat=0;;#}" />
                                <TextBlock
                                    FontFamily="arial" FontSize="16" HorizontalAlignment="Center"
                                    Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                    VerticalAlignment="Center"
                                    Text="{Binding NewProd.Dent,StringFormat=0;;#}" />
                            </Grid>
                        </DockPanel>
                    </Border>

                    <Border Grid.Row="1" Grid.Column="0" BorderBrush="Black" BorderThickness="1 0 1 1">
                        <DockPanel>
                            <TextBlock FontSize="14" FontFamily="arial" HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                Vitesse ML/MIN :
                            </TextBlock>
                            <Grid>
                                <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                           Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                           VerticalAlignment="Center" Text="{Binding Vitesse}" />
                                <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                           Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                           VerticalAlignment="Center" Text="{Binding NewProd.DuitageID.Vitesse}" />
                            </Grid>
                        </DockPanel>
                    </Border>

                    <Border Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="0 0 1 1">
                        <Grid>
                            <DockPanel Visibility="{Binding IsFicheTissage,Converter={StaticResource BoolToVis}}">
                                <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                           VerticalAlignment="Center">
                                    Peigne :
                                </TextBlock>
                                <Grid HorizontalAlignment="Center">
                                    <ComboBox PreviewTextInput="CheckForDoubleValue"
                                              DataObject.Pasting="Pasting_CheckDoubleValue" BorderBrush="Transparent"
                                              Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                              FontSize="16" Width="50" FontFamily="arial"
                                              HorizontalContentAlignment="Center"
                                              VerticalContentAlignment="Center"
                                              ItemsSource="{Binding ListPeigne}"
                                              DisplayMemberPath="Nombre"
                                              SelectedItem="{Binding NewProd.PeigneObj}" />
                                    <TextBlock
                                        Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                        FontSize="16" Width="50" FontFamily="arial" HorizontalAlignment="Right"
                                        TextAlignment="Center"
                                        VerticalAlignment="Center"
                                        Text="{Binding NewProd.PeigneObj.Nombre,StringFormat=0.0;;#}" />
                                </Grid>
                                <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Left"
                                           VerticalAlignment="Center">
                                    D/CM
                                </TextBlock>
                            </DockPanel>
                            <DockPanel Visibility="{Binding IsFicheCrochetage,Converter={StaticResource BoolToVis}}">
                                <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                           VerticalAlignment="Center">
                                    Duitage Gomme :
                                </TextBlock>
                                <Grid HorizontalAlignment="Stretch">

                                    <ComboBox Cursor="Hand" FontSize="16" FontFamily="arial"
                                              Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                              HorizontalContentAlignment="Center" VerticalContentAlignment="Center"

                                              HorizontalAlignment="Stretch" ItemsSource="{Binding DuitageGoList}"
                                              DisplayMemberPath="Duitage" SelectedItem="{Binding NewProd.DuitageGomme}" />
                                    <TextBlock
                                        Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                        FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                        TextAlignment="Center"
                                        VerticalAlignment="Center"
                                        Text="{Binding NewProd.DuitageGomme.Duitage,StringFormat=0.0;;#}" />

                                </Grid>
                            </DockPanel>
                        </Grid>

                    </Border>

                    <Border Grid.Row="1" Grid.Column="2" BorderBrush="Black" BorderThickness="0 0 1 1">
                        <DockPanel 
                            Visibility="{Binding IsFicheTressage,Converter={StaticResource BoolToVis},ConverterParameter={StaticResource BoolParam}}"
                            >
                            <TextBlock FontSize="16" FontFamily="arial" HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                Machine :
                            </TextBlock>
                            <Grid>
                                <ComboBox FontSize="16" IsTextSearchEnabled="True" TextSearch.TextPath="Num"


                                          FontFamily="arial" HorizontalContentAlignment="Stretch"
                                          VerticalContentAlignment="Center" Cursor="Hand" DisplayMemberPath="Name"
                                          Visibility="{Binding EditDate,Converter={StaticResource BoolToVis}}"
                                          HorizontalAlignment="Stretch"

                                          ScrollViewer.VerticalScrollBarVisibility="Visible"
                                          SelectedItem="{Binding SelectedMachine}" ItemsSource="{Binding MachineList}" />
                                <TextBlock FontSize="16"
                                           Visibility="{Binding DisplayDate,Converter={StaticResource BoolToVis}}"
                                           FontFamily="arial" HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Text="{Binding NewProd.DuitageID.Machine.Name}" />
                            </Grid>
                        </DockPanel>
                    </Border>


                </Grid>

            </Grid>

            <Grid IsEnabled="{Binding EnableEditing}" Grid.Row="2" Grid.RowSpan="2">

                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.5*" />
                    <ColumnDefinition Width="2*" />
                    <ColumnDefinition Width="5*" />
                    <ColumnDefinition Width="1*" />
                    <ColumnDefinition Width="0.8*" />
                    <ColumnDefinition Width="0.8*" />
                    <ColumnDefinition Width="1*" />
                    <ColumnDefinition Width="2*" />
                    <ColumnDefinition Width="3*" />
                </Grid.ColumnDefinitions>

                <Button Visibility="{Binding BtnVis,Converter={StaticResource BoolToVis}}" BorderThickness="0"
                        Command="{Binding CmdAddComp1}" Grid.Row="1"
                        Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}">

                    <Image Source="{Binding ChangeImageBtn1}" Cursor="Hand" />
                </Button>

                <Button Visibility="{Binding BtnVis,Converter={StaticResource BoolToVis}}" BorderThickness="0"
                        Command="{Binding CmdAddComp2}" Grid.Row="2"
                        Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}">

                    <Image Source="{Binding ChangeImageBtn2}" Cursor="Hand" />
                </Button>
                <Button Visibility="{Binding BtnVis,Converter={StaticResource BoolToVis}}" BorderThickness="0"
                        Command="{Binding CmdAddComp3}" Grid.Row="3"
                        Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}">

                    <Image Source="{Binding ChangeImageBtn3}" Cursor="Hand" />
                </Button>
                <Button Visibility="{Binding BtnVis,Converter={StaticResource BoolToVis}}" BorderThickness="0"
                        Command="{Binding CmdAddComp4}" Grid.Row="4"
                        Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}">

                    <Image Source="{Binding ChangeImageBtn4}" Cursor="Hand" />
                </Button>
                <Button Visibility="{Binding BtnVis,Converter={StaticResource BoolToVis}}" BorderThickness="0"
                        Command="{Binding CmdAddComp5}" Grid.Row="5"
                        Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}">

                    <Image Source="{Binding ChangeImageBtn5}" Cursor="Hand" />
                </Button>
                <Button Visibility="{Binding BtnVis,Converter={StaticResource BoolToVis}}" BorderThickness="0"
                        Command="{Binding CmdAddComp6}" Grid.Row="6"
                        Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}">

                    <Image Source="{Binding ChangeImageBtn6}" Cursor="Hand" />
                </Button>
                <Button Visibility="{Binding BtnVis,Converter={StaticResource BoolToVis}}" BorderThickness="0"
                        Command="{Binding CmdAddComp7}" Grid.Row="7"
                        Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}">

                    <Image Source="{Binding ChangeImageBtn7}" Cursor="Hand" />
                </Button>
